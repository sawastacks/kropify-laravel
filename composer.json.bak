{
    "name": "mberecall/kropify-laravel-test",
    "description": "Kropify is a tool that can be integrated into Laravel framework 8+ for the purpose of giving users easy way to crop their profile pictures and cover images.",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "kropify", 
        "kropify laravel", 
        "laravel kropify", 
        "laravel-kropify",
        "laravel",
        "php",
        "cropper",
        "crop",
        "cropping",
        "mberecall",
        "irebe library",
        "ijabo crop tool",
        "ijabocroptool"    
    ],
    "homepage": "https://github.com/mberecall/kropify-laravel/",
    "authors": [
        {
            "name": "MB'DUSENGE Callixte (mberecall)",
            "email": "mberecall@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.2.0",
        "intervention/image": "^2.7"
    },
    "autoload": {
        "psr-4": {
            "Mberecall\\Kropify\\": "src/"
        },
        "files": [
            "./src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mberecall\\Kropify\\KropifyServiceProvider"
            ],
            "aliases": {
                "Kropify": "Mberecall\\Kropify\\Facades\\Kropify"
            }
                
        }
    }
}
